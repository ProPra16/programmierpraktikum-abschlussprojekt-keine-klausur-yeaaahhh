    </Text>
    <Text font="$x1" layoutX="300.0" layoutY="116.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Description" />
    <Button fx:id="selectButton" layoutX="692.0" layoutY="555.0" mnemonicParsing="false" onAction="#loadExcersise" text="Select" />
    <Text layoutX="40.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="TDDT">
      <font>
        <Font name="Arial" size="35.0" />
      </font>
    </Text>
